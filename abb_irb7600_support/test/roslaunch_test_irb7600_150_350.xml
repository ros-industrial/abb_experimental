<launch>
  <arg name="ip_str" value="127.0.0.1" />

  <group ns="load_irb_7600_150__">
    <include file="$(find abb_irb_7600_150_support)/launch/load_irb_7600_150.launch"/>
  </group>

  <group ns="test_irb_7600_150__">
    <include file="$(find abb_irb_7600_150_support)/launch/test_irb2400.launch"/>
  </group>

  <group ns="robot_interface_download_irb_7600_150__">
    <include file="$(find abb_irb_7600_150_support)/launch/robot_interface_download_irb_7600_150.launch">
      <arg name="robot_ip" value="$(arg ip_str)" />
    </include>
  </group>

  <group ns="robot_state_visualize_irb_7600_150__">
    <include file="$(find abb_irb_7600_150_support)/launch/robot_state_visualize_irb_7600_150.launch">
      <arg name="robot_ip" value="$(arg ip_str)" />
    </include>
  </group>
</launch>
